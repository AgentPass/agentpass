{
  "name": "workspace",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "sdk/*",
    "libs/*",
    "apps/*"
  ],
  "scripts": {
    "gen:api": "./scripts/api/gen.sh",
    "gen:dockerfile": "node dockerfile-gen.js",
    "ls:apps": "nx show projects --type app",
    "sdk:storybook:gzip": "yarn sdk:run gzipper compress ./storybook-static ./storybook-static/gzip --verbose --output-file-format [filename].[ext]",
    "install-git-hooks": "cp scripts/git-hooks/pre-commit ../.git/hooks/pre-commit && chmod +x ../.git/hooks/pre-commit",
    "sync-and-lint": "nx sync && nx run-many -t lint --all --fix --projects=\"tag:scope:extension,tag:scope:sdk,tag:scope:agentbridge\" && echo \"formatting...\" && nx format:write --all",
    "postinstall": "scripts/remove-fingerprint.sh"
  },
